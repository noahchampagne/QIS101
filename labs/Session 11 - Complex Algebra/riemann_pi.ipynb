    "# riemann_pi.ipynb\n",
    "# Cell 1\n",
    "\n",
    "import numpy as np\n",
    "import sympy\n",
    "from IPython.display import Math\n",
    "\n",
    "for n in range(1, 10):\n",
    "\n",
    "    # Use sympy to calculate the Riemann Pi for 10^n\n",
    "    riemann_pi = int(sympy.primepi(10**n))\n",
    "\n",
    "    display(Math(rf\"\\pi(10^{n}) = {riemann_pi:,}\"))"
   ]
  }
